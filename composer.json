{
    "name": "drupal/quiz_maker",
    "license": "GPL-2.0-or-later",
    "description": "Provide functionality to Create engaging quizzes.",
    "type": "drupal-module",
    "authors": [
        {
            "name": "Roman Chekhaniuk",
            "email": "roman.chekhaniuk@lembergsolutions.com"
        }
    ],
    "require": {
      "php": ">=8.1"
    },
    "require-dev": {
        "mglaman/phpstan-drupal": "^1.2",
        "phpro/grumphp": "^2.4",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "drupal/coder": "^8.3"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
